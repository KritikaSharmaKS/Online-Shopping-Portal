    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>  
            <% if (product.length) { %>
                <ul>
                    <% product.forEach(p => { %>
                        <li><%= p.productData.title %>(<%= p.qty %>)</li>
                    <% }); %>    
                </ul>
            <% } else { %>   
                <h1> No Products Found!</h1>
            <% } %>       
        </main>
        <%- include('../includes/end.ejs') %>